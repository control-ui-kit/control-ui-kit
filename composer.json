{
    "name": "control-ui-kit/control-ui-kit",
    "description": "Laravel blade component UI Kit",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Chambers"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/filesystem": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "nesbot/carbon": "^2.38",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ControlUIKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ControlUIKit\\ControlUIKitServiceProvider"
            ]
        }
    }
}
