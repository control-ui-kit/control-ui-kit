{
    "name": "control-ui-kit/control-ui-kit",
    "description": "Laravel blade component UI Kit",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Chambers"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "illuminate/filesystem": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0",
        "league/uri": "^6.4",
        "league/uri-components": "^2.3",
        "facade/ignition-contracts": "^1.0",
        "nesbot/carbon": "^2.38"
    },
    "require-dev": {
        "gajus/dindent": "^2.0",
        "league/commonmark": "^1.4",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ControlUIKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ControlUIKit\\ControlUIKitServiceProvider"
            ]
        }
    },
    "suggest": {
        "league/commonmark": "Required to use the markdown component (^1.4)."
    },
    "config": {
        "sort-packages": true
    }
}
